{
  "name": "server",
  "version": "1.2.1",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles",
    "test:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.js --forceExit --detectOpenHandles",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:watch:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js --watch",
    "test:watch:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.js --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --forceExit --detectOpenHandles",
    "test:coverage:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js --coverage",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:clean": "node scripts/clean-database.js",
    "db:clean:silent": "node scripts/clean-database.js --silent",
    "workflow": "node scripts/full-workflow.js",
    "workflow:silent": "node scripts/full-workflow.js --silent",
    "etl:help": "node scripts/etl-help.js",
    "etl:validate": "node scripts/validate-etl.js",
    "etl:load": "node scripts/etl-load-data.js",
    "etl:load:usa": "node scripts/etl-load-usa-data.js",
    "etl:test": "node scripts/etl-test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.13.0",
    "nodemon": "^3.1.9",
    "sequelize": "^6.37.6",
    "sharp": "^0.34.5",
    "uuid": "^11.0.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
