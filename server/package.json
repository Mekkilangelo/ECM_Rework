{
  "name": "server",
  "version": "1.1.2",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles",
    "test:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js",
    "test:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.js --forceExit --detectOpenHandles",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:watch:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js --watch",
    "test:watch:integration": "cross-env NODE_ENV=test jest --config jest.integration.config.js --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --forceExit --detectOpenHandles",
    "test:coverage:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js --coverage",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.13.0",
    "nodemon": "^3.1.9",
    "sequelize": "^6.37.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0"
  }
}
